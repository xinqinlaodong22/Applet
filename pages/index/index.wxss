.index_swiper swiper {
  width: 100vw;
  height: calc(100vw * 340 / 750);
}
.index_swiper image {
  width: 100%;
}
.index_cate {
  display: flex;
}
.index_cate navigator {
  flex: 1;
  padding: 20rpx;
}
.index_cate image {
  width: 100%;
}
.index_floor .index_group .floor_title {
  padding: 10rpx 0;
}
.index_floor .index_group .floor_title image {
  width: 100%;
}
.index_floor .index_group .floor_list {
  overflow: hidden;
}
.index_floor .index_group .floor_list navigator {
  float: left;
  width: 33.33%;
}
.index_floor .index_group .floor_list navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  padding-left: 10rpx;
}
.index_floor .index_group .floor_list navigator:nth-last-child(3) {
  padding-bottom: 10rpx;
}
.index_floor .index_group .floor_list navigator image {
  width: 100%;
  height: 100%;
}
